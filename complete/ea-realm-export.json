{
  "realm": "ea",
  "enabled": true,
  "displayName": "EA Realm",
  "accessTokenLifespan": 28800,
  "ssoSessionIdleTimeout": 28800,
  "ssoSessionMaxLifespan": 28800,
  "offlineSessionIdleTimeout": 2592000,
  "roles": {
    "realm": [
      { "name": "user" },
      { "name": "admin" }
    ],
    "client": {
      "web-app": [
        { "name": "user" },
        { "name": "admin" }
      ]
    }
  },
  "clients": [
    {
      "clientId": "web-app",
      "enabled": true,
      "publicClient": true,
      "directAccessGrantsEnabled": true,
      "standardFlowEnabled": true,
      "redirectUris": ["*"],
      "webOrigins": ["*"],
      "protocol": "openid-connect",
      "fullScopeAllowed": true
    }
  ],
  "users": [
    {
      "username": "admin",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Admin",
      "lastName": "User",
      "email": "admin@ea.local",
      "credentials": [
        {
          "type": "password",
          "value": "admin",
          "temporary": false
        }
      ],
      "realmRoles": ["user", "admin"],
      "clientRoles": {
        "web-app": ["user", "admin"]
      }
    },
    {
      "username": "user",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Test",
      "lastName": "User",
      "email": "user@ea.local",
      "credentials": [
        {
          "type": "password",
          "value": "user",
          "temporary": false
        }
      ],
      "realmRoles": ["user"],
      "clientRoles": {
        "web-app": ["user"]
      }
    }
  ]
}
